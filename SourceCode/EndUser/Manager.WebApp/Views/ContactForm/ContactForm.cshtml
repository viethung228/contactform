                                {
                                    var v = p.Name;
                                    object[] descriptionAttrs = p.GetCustomAttributes(typeof(DescriptionAttribute), false);

                                    DescriptionAttribute description = (DescriptionAttribute)descriptionAttrs[0];

                                    <option class="NengoYear" value="@typeof(EnumJapaneseImperialNengo).GetField(v).GetValue(null)">@description.Description</option>
                                }
                            </select>

